#ifndef Bullet_h
#define Bullet_h

class Bullet
{
private:
    double x;
    double y;

public:
    ~Bullet();

    void toxy(int x, int y);
    double getBx();
    double getBy();
    
    void setx(double a);
    void sety(double a);

    void show();
    void Dshow();

    void move();



};
#endif
